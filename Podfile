# platform :ios, '9.0'

target 'Extensions' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Extensions

  def testing_pods
    pod 'Quick'
    pod 'Nimble'
    pod 'KIF'
  end

  target 'ExtensionsTests' do
    inherit! :search_paths
    testing_pods
    #Pods for testing
  end

  target 'ExtensionsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
